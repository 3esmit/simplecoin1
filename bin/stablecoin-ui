#!/usr/bin/env node
var express = require("express")
var morgan = require("morgan")

var app = express()
var port = process.env.PORT || 3000

app.use(express.static("public"))
app.use(morgan("dev"))

app.get("/coins/:address", () => {
  
})

app.listen(port, () => {
  console.log(`Serving app at http://localhost:${port}`)
})
